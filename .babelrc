{
  "presets": [
    "flow",
    "env"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "comments": false,
  "ignore": [
    "interface.js"
  ],
  "env": {
    "production": {
      "presets": [
        ["env", {
          "targets": {
            "browsers": ["last 2 versions"]
          }
        }],
        "minify",
        "flow"
      ],
      "plugins": [
        "transform-class-properties"
      ]
    }
  }
}
